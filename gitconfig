[core]
	whitespace = warn
	excludesfile = ~/.global_gitignore

[http]
  proxy = fwdproxy:8080

[https]
  proxy = fwdproxy:8080

[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true

[alias]
	rbc = rebase --continue
	l = log --oneline --decorate --graph
	d = diff
