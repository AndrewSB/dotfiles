Host github.com
	AddKeysToAgent yes
	IgnoreUnknown UseKeychain
	UseKeychain yes
	IdentityFile ~/.ssh/id_ed25519
	# Often, public wifi has Port 22 blocked. 443 is blocked less often
	Port 443

